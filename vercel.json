{
    "version": 2,
    "builds": [
      {
        "src": "fem/wsgi.py",
        "use": "@ardnt/now-python-wsgi",
        "config": {
          "maxLambdaSize": "30mb"
        }
      },
      {
        "src": "build_files.sh",
        "use": "@vercel/static",
        "config": {
          "distDir": "staticfiles"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "fem/wsgi.py"
      }
    ]
  }